<?xml version="1.0"?>

<metacity_theme>
<info>
  	<name>ACID</name>
  	<author>Padster/SixSixFive</author>
  	<copyright>GPL</copyright>
  	<date>19 August 2014</date>
  	<description>A Metacity theme to go with the ACID suite, or any other theme you like</description>
</info>

<!-- Based on Hope by Reda Lazri(rAX) -->

<!--
Constants for the colours used. 
Uses wm_x from GTK theme if present (e.g. Numix, Ceti, etc.) otherwise, defaults to default colours
-->

<constant name="Focused" value="gtk:custom(wm_title_focused,#a6e028)"/> <!-- #ff4d65 makes a nice pink colour -->
<constant name="UnFocused" value="gtk:custom(wm_title_unfocused,#888)"/>
<constant name="WindowBg" value="gtk:custom(wm_bg,#333)"/>



<!-- General window layout -->
<frame_geometry name="frame_geometry_normal" title_scale="medium" rounded_top_left="false" rounded_top_right="false"  rounded_bottom_left="false" rounded_bottom_right="false">
 	<distance name="left_width" value="2"/>
	<distance name="right_width" value="2"/>
	<distance name="bottom_height" value="2"/>
	<distance name="left_titlebar_edge" value="2"/>
	<distance name="right_titlebar_edge" value="5"/>
	<distance name="button_width" value="13"/>
	<distance name="button_height" value="13"/>
	<distance name="title_vertical_pad" value="5"/>
	<border name="title_border" left="0" right="0" top="0" bottom="0"/>
	<border name="button_border" left="3" right="0" top="0" bottom="0"/>
</frame_geometry>

<frame_geometry name="geometry_maximized" rounded_top_left="false" rounded_top_right="false" rounded_bottom_left="false" rounded_bottom_right="false">
	<distance name="left_width" value="0"/>
	<distance name="right_width" value="0"/>
	<distance name="bottom_height" value="0"/>
	<distance name="left_titlebar_edge" value="2"/>
  	<distance name="right_titlebar_edge" value="5"/>
  	<distance name="button_width" value="13"/>
  	<distance name="button_height" value="13"/>
  	<distance name="title_vertical_pad" value="5"/>
  	<border name="title_border" left="0" right="0" top="0" bottom="0"/>
 	<border name="button_border" left="3" right="0" top="0" bottom="0"/>
</frame_geometry>
<!-- No title (e.g. Modal) -->
<frame_geometry name="border" has_title="false">
	<distance name="left_width" value="2"/>
	<distance name="right_width" value="2"/>
	<distance name="bottom_height" value="2"/>
	<distance name="left_titlebar_edge" value="2"/>
  	<distance name="right_titlebar_edge" value="5"/>
  	<distance name="button_width" value="0"/>
  	<distance name="button_height" value="0"/>
  	<distance name="title_vertical_pad" value="0"/>
  	<border name="title_border" left="0" right="0" top="0" bottom="0"/>
 	<border name="button_border" left="3" right="0" top="0" bottom="0"/>
</frame_geometry>

<draw_ops name="blank">
	
</draw_ops>

<draw_ops name="draw_title_text_normal">
	<title color="Focused" x="3 `max` (width-title_width)/2" y="0 `max` ((height-title_height)/2)"/>
</draw_ops>

<draw_ops name="draw_title_text_inactive">
	<title color="UnFocused" x="3 `max` (width-title_width)/2" y="0 `max` ((height-title_height)/2)"/>
</draw_ops>

<draw_ops name="draw_title">
	<rectangle color="#232323" filled="true"
	           x="0" y="0"
	           width="width"
	           height="height"/>
	<rectangle color="#404040" filled="true"
	           x="1" y="1"
	           width="width - 2"
	           height="height"/>
	<rectangle color="WindowBg" filled="true"
	           x="2" y="2"
	           width="width - 4"
	           height="height"/>
</draw_ops>

<draw_ops name="draw_title_inactive">
	<rectangle color="#232323" filled="true"
	           x="0" y="0"
	           width="width"
	           height="height"/>
	<rectangle color="WindowBg" filled="true"
	           x="1" y="1"
	           width="width - 2"
	           height="height"/>
</draw_ops>

<draw_ops name="draw_title_maximized">
	<rectangle color="WindowBg" filled="true"
	           x="0" y="0"
	           width="width"
	           height="height"/>
	<line color="#404040"
        	   x1="0"
        	   y1="1"
        	   x2="width"
        	   y2="1"
        	   width="1"/>
	<line color="#232323"
        	   x1="0"
        	   y1="0"
        	   x2="width"
        	   y2="0"
        	   width="1"/>
</draw_ops>

<draw_ops name="draw_title_maximized_inactive">
	<rectangle color="WindowBg" filled="true"
	           x="0" y="0"
	           width="width"
	           height="height"/>
	<line color="#232323"
        	   x1="0"
        	   y1="0"
        	   x2="width"
        	   y2="0"
        	   width="1"/>
</draw_ops>

<!-- Window Frames -->

<draw_ops name="frame-bottom-focused">
<rectangle color="#232323" filled="true"
	   x="0" y="0"
	   width="width"
	   height="height"/>
<line color="#404040"
        x1="1"
        y1="0"
        x2="width - 1"
        y2="0"
        width="1"/>
</draw_ops>
<draw_ops name="frame-bottom-unfocused">
<rectangle color="#232323" filled="true"
	   x="0" y="0"
	   width="width"
	   height="height"/>
<line color="WindowBg"
        x1="1"
        y1="0"
        x2="width - 1"
        y2="0"
        width="1"/>
</draw_ops>

<draw_ops name="frame-left-focused">
<line color="#232323"
        x1="0"
        y1="0"
        x2="0"
        y2="height"
        width="1"/>
<line color="#404040"
        x1="1"
        y1="0"
        x2="1"
        y2="height"
        width="1"/>
</draw_ops>
<draw_ops name="frame-left-unfocused">
<line color="#232323"
        x1="0"
        y1="0"
        x2="0"
        y2="height"
        width="1"/>
<line color="WindowBg"
        x1="1"
        y1="0"
        x2="1"
        y2="height"
        width="1"/>
</draw_ops>

<draw_ops name="frame-right-focused">
<line color="#232323"
        x1="1"
        y1="0"
        x2="1"
        y2="height"
        width="1"/>
<line color="#404040"
        x1="0"
        y1="0"
        x2="0"
        y2="height"
        width="1"/>
</draw_ops>
<draw_ops name="frame-right-unfocused">
<line color="#232323"
        x1="1"
        y1="0"
        x2="1"
        y2="height"
        width="1"/>
<line color="WindowBg"
        x1="0"
        y1="0"
        x2="0"
        y2="height"
        width="1"/>
</draw_ops>

<!-- BUTTONS -->
	<draw_ops name="menu_focused_normal">
		<line color="Focused"
        x1="width/2 - 2"
        y1="height/2 - 2"
        x2="width/2 + 3"
        y2="height/2 - 2"
        width="1"/>
		<line color="Focused"
        x1="width/2 - 2"
        y1="height/2"
        x2="width/2 + 2"
        y2="height/2"
        width="1"/>
        <line color="Focused"
        x1="width/2 - 2"
        y1="height/2 + 2"
        x2="width/2 + 3"
        y2="height/2 + 2"
        width="1"/>
	</draw_ops>
	<draw_ops name="menu_focused_prelight">
  	 <include name="menu_focused_normal"/>
	</draw_ops>
	<draw_ops name="menu_focused_pressed">
		<include name="menu_focused_normal"/>
	</draw_ops>
	<draw_ops name="menu_unfocused_normal">
  	 	<line color="UnFocused"
        x1="width/2 - 2"
        y1="height/2 - 2"
        x2="width/2 + 3"
        y2="height/2 - 2"
        width="1"/>
		<line color="UnFocused"
        x1="width/2 - 2"
        y1="height/2"
        x2="width/2 + 2"
        y2="height/2"
        width="1"/>
        <line color="UnFocused"
        x1="width/2 - 2"
        y1="height/2 + 2"
        x2="width/2 + 3"
        y2="height/2 + 2"
        width="1"/>
	</draw_ops>
	<draw_ops name="menu_unfocused_prelight">
   		<include name="menu_unfocused_normal"/>
	</draw_ops>
	<draw_ops name="menu_unfocused_pressed">
		<include name="menu_unfocused_normal"/>
	</draw_ops>
	<draw_ops name="minimize_focused_normal">
		<rectangle color="Focused" x="width/2 - 2" y="height/2 - 1" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 - 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2" y="height/2 + 1" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 + 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 + 2" y="height/2 - 1" width="1" height="1" filled="true"/>
	</draw_ops>
	<draw_ops name="minimize_focused_prelight">
		<include name="minimize_focused_normal"/>
	</draw_ops>
	<draw_ops name="minimize_focused_pressed">
		<include name="minimize_focused_normal"/>
	</draw_ops>
	<draw_ops name="minimize_unfocused_normal">
		<rectangle color="UnFocused" x="width/2 - 2" y="height/2 - 1" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 - 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2" y="height/2 + 1" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 + 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 + 2" y="height/2 - 1" width="1" height="1" filled="true"/>
	</draw_ops>
	<draw_ops name="minimize_unfocused_prelight">
		<include name="minimize_unfocused_normal"/>
	</draw_ops>
	<draw_ops name="minimize_unfocused_pressed">
		<include name="minimize_unfocused_normal"/>
	</draw_ops>
	<draw_ops name="maximize_focused_normal">
		<rectangle color="Focused" x="width/2 - 2" y="height/2 + 1" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 - 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2" y="height/2 - 1" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 + 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 + 2" y="height/2 + 1" width="1" height="1" filled="true"/>
	</draw_ops>
	<draw_ops name="maximize_focused_prelight">
		<include name="maximize_focused_normal"/>
	</draw_ops>
	<draw_ops name="maximize_focused_pressed">
		<include name="maximize_focused_normal"/>
	</draw_ops>
	<draw_ops name="maximize_unfocused_normal">
		<rectangle color="UnFocused" x="width/2 - 2" y="height/2 + 1" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 - 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2" y="height/2 - 1" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 + 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 + 2" y="height/2 + 1" width="1" height="1" filled="true"/>
	</draw_ops>
	<draw_ops name="maximize_unfocused_prelight">
		<include name="maximize_unfocused_normal"/>
	</draw_ops>
	<draw_ops name="maximize_unfocused_pressed">
		<include name="maximize_unfocused_normal"/>
	</draw_ops>
	<draw_ops name="unmaximize_focused_normal"> <!--TODO - tweak this colour-->
		<rectangle color="#f0f" x="width/2 - 2" y="height/2 + 1" width="1" height="1" filled="true"/>
		<rectangle color="#f0f" x="width/2 - 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="#f0f" x="width/2" y="height/2 - 1" width="1" height="1" filled="true"/>
		<rectangle color="#f0f" x="width/2 + 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="#f0f" x="width/2 + 2" y="height/2 + 1" width="1" height="1" filled="true"/>
	</draw_ops>
	<draw_ops name="unmaximize_focused_prelight">
		<include name="unmaximize_focused_normal"/>
	</draw_ops>
	<draw_ops name="unmaximize_focused_pressed">
		<include name="unmaximize_focused_normal"/>
	</draw_ops>
	<draw_ops name="unmaximize_unfocused_normal">
		<rectangle color="#f0f" x="width/2 - 2" y="height/2 + 1" width="1" height="1" filled="true"/>
		<rectangle color="#f0f" x="width/2 - 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="#f0f" x="width/2" y="height/2 - 1" width="1" height="1" filled="true"/>
		<rectangle color="#f0f" x="width/2 + 1" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="#f0f" x="width/2 + 2" y="height/2 + 1" width="1" height="1" filled="true"/>
	</draw_ops>
	<draw_ops name="unmaximize_unfocused_prelight">
		<include name="unmaximize_unfocused_normal"/>
	</draw_ops>
	<draw_ops name="unmaximize_unfocused_pressed">
		<include name="unmaximize_unfocused_normal"/>
	</draw_ops>
	<draw_ops name="close_focused_normal">

		<rectangle color="Focused" x="width/2 - 2" y="height/2 - 2" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 - 1" y="height/2 - 1" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 + 1" y="height/2 - 1" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 + 2" y="height/2 - 2" width="1" height="1" filled="true"/>

		<rectangle color="Focused" x="width/2 - 1" y="height/2 + 1" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 - 2" y="height/2 + 2" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 + 1" y="height/2 + 1" width="1" height="1" filled="true"/>
		<rectangle color="Focused" x="width/2 + 2" y="height/2 + 2" width="1" height="1" filled="true"/>
	</draw_ops>
	<draw_ops name="close_focused_prelight">
		<include name="close_focused_normal"/>
	</draw_ops>
	<draw_ops name="close_focused_pressed">
		<include name="close_focused_normal"/>
	</draw_ops>
	<draw_ops name="close_unfocused_normal">
		<rectangle color="UnFocused" x="width/2 - 2" y="height/2 - 2" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 - 1" y="height/2 - 1" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2" y="height/2" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 + 1" y="height/2 - 1" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 + 2" y="height/2 - 2" width="1" height="1" filled="true"/>

		<rectangle color="UnFocused" x="width/2 - 1" y="height/2 + 1" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 - 2" y="height/2 + 2" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 + 1" y="height/2 + 1" width="1" height="1" filled="true"/>
		<rectangle color="UnFocused" x="width/2 + 2" y="height/2 + 2" width="1" height="1" filled="true"/>
	</draw_ops>
	<draw_ops name="close_unfocused_prelight">
		<include name="close_unfocused_normal"/>
	</draw_ops>
	<draw_ops name="close_unfocused_pressed">
		<include name="close_unfocused_normal"/>
	</draw_ops>

<!-- FRAME STYLE -->
<frame_style name="normal_focused" geometry="frame_geometry_normal">
	<piece position="title" draw_ops="draw_title_text_normal"/>
   	<piece position="titlebar" draw_ops="draw_title"/>
	<piece position="left_edge" draw_ops="frame-left-focused"/>
	<piece position="right_edge" draw_ops="frame-right-focused"/>
   	<piece position="bottom_edge" draw_ops="frame-bottom-focused"/>
	<button function="menu" state="normal" draw_ops="menu_focused_normal"/>
	<button function="menu" state="prelight" draw_ops="menu_focused_prelight"/>
	<button function="menu" state="pressed" draw_ops="menu_focused_pressed"/> 
	<button function="minimize" state="normal" draw_ops="minimize_focused_normal"/>
	<button function="minimize" state="prelight" draw_ops="minimize_focused_prelight"/>
	<button function="minimize" state="pressed" draw_ops="minimize_focused_pressed"/>
	<button function="maximize" state="normal" draw_ops="maximize_focused_normal"/>
	<button function="maximize" state="prelight" draw_ops="maximize_focused_prelight"/>
	<button function="maximize" state="pressed" draw_ops="maximize_focused_pressed"/>
	<button function="close" state="normal" draw_ops="close_focused_normal"/>
	<button function="close" state="prelight" draw_ops="close_focused_prelight"/>
	<button function="close" state="pressed" draw_ops="close_focused_pressed"/>
	<!-- Need these for the theme to work -->
  <button function="shade" state="normal" draw_ops="blank"/>
  <button function="shade" state="prelight" draw_ops="blank"/>
  <button function="shade" state="pressed" draw_ops="blank"/>
  <button function="unshade" state="normal" draw_ops="blank"/>
  <button function="unshade" state="prelight" draw_ops="blank"/>
  <button function="unshade" state="pressed" draw_ops="blank"/>
  <button function="above" state="normal" draw_ops="blank"/>
  <button function="above" state="prelight" draw_ops="blank"/>
  <button function="above" state="pressed" draw_ops="blank"/>
  <button function="unabove" state="normal" draw_ops="blank"/>
  <button function="unabove" state="prelight" draw_ops="blank"/>
  <button function="unabove" state="pressed" draw_ops="blank"/>
  <button function="stick" state="normal" draw_ops="blank"/>
  <button function="stick" state="prelight" draw_ops="blank"/>
  <button function="stick" state="pressed" draw_ops="blank"/>
  <button function="unstick" state="normal" draw_ops="blank"/>
  <button function="unstick" state="prelight" draw_ops="blank"/>
  <button function="unstick" state="pressed" draw_ops="blank"/>
</frame_style>

<frame_style name="normal_unfocused" geometry="frame_geometry_normal">
	<piece position="title" draw_ops="draw_title_text_inactive"/>
   	<piece position="titlebar" draw_ops="draw_title_inactive"/>
	<piece position="left_edge" draw_ops="frame-left-unfocused"/>
	<piece position="right_edge" draw_ops="frame-right-unfocused"/>
   	<piece position="bottom_edge" draw_ops="frame-bottom-unfocused"/>
	<button function="menu" state="normal" draw_ops="menu_unfocused_normal"/>
	<button function="menu" state="prelight" draw_ops="menu_unfocused_prelight"/>
	<button function="menu" state="pressed" draw_ops="menu_unfocused_pressed"/>
	<button function="minimize" state="normal" draw_ops="minimize_unfocused_normal"/>
	<button function="minimize" state="prelight" draw_ops="minimize_unfocused_prelight"/>
	<button function="minimize" state="pressed" draw_ops="minimize_unfocused_pressed"/>
	<button function="maximize" state="normal" draw_ops="maximize_unfocused_normal"/>
	<button function="maximize" state="prelight" draw_ops="maximize_unfocused_prelight"/>
	<button function="maximize" state="pressed" draw_ops="maximize_unfocused_pressed"/>
	<button function="close" state="normal" draw_ops="close_unfocused_normal"/>
	<button function="close" state="prelight" draw_ops="close_unfocused_prelight"/>
	<button function="close" state="pressed" draw_ops="close_unfocused_pressed"/>
	<!-- Need these for the theme to work -->
  <button function="shade" state="normal" draw_ops="blank"/>
  <button function="shade" state="prelight" draw_ops="blank"/>
  <button function="shade" state="pressed" draw_ops="blank"/>
  <button function="unshade" state="normal" draw_ops="blank"/>
  <button function="unshade" state="prelight" draw_ops="blank"/>
  <button function="unshade" state="pressed" draw_ops="blank"/>
  <button function="above" state="normal" draw_ops="blank"/>
  <button function="above" state="prelight" draw_ops="blank"/>
  <button function="above" state="pressed" draw_ops="blank"/>
  <button function="unabove" state="normal" draw_ops="blank"/>
  <button function="unabove" state="prelight" draw_ops="blank"/>
  <button function="unabove" state="pressed" draw_ops="blank"/>
  <button function="stick" state="normal" draw_ops="blank"/>
  <button function="stick" state="prelight" draw_ops="blank"/>
  <button function="stick" state="pressed" draw_ops="blank"/>
  <button function="unstick" state="normal" draw_ops="blank"/>
  <button function="unstick" state="prelight" draw_ops="blank"/>
  <button function="unstick" state="pressed" draw_ops="blank"/>
</frame_style>

<frame_style name="normal-shaded-focused" geometry="frame_geometry_normal" parent="normal_focused">
  <piece position="title" draw_ops="draw_title_text_normal"/>
  <piece position="titlebar" draw_ops="draw_title"/>
</frame_style>

<frame_style name="normal-shaded-unfocused" geometry="frame_geometry_normal" parent="normal_unfocused">
  <piece position="title" draw_ops="draw_title_text_inactive"/>
  <piece position="titlebar" draw_ops="draw_title_inactive"/>
</frame_style>

<frame_style name="maximized_focused" geometry="geometry_maximized" parent="normal_focused">
	<piece position="title" draw_ops="draw_title_text_normal"/>
   	<piece position="titlebar" draw_ops="draw_title_maximized"/>
	<button function="maximize" state="normal" draw_ops="unmaximize_focused_normal"/>
	<button function="maximize" state="prelight" draw_ops="unmaximize_focused_prelight"/>
	<button function="maximize" state="pressed" draw_ops="unmaximize_focused_pressed"/>
</frame_style>

<frame_style name="maximized_unfocused" geometry="geometry_maximized" parent="normal_unfocused">
	<piece position="title" draw_ops="draw_title_text_inactive"/>
   	<piece position="titlebar" draw_ops="draw_title_maximized_inactive"/>
	<button function="maximize" state="normal" draw_ops="unmaximize_unfocused_normal"/>
	<button function="maximize" state="prelight" draw_ops="unmaximize_unfocused_prelight"/>
	<button function="maximize" state="pressed" draw_ops="unmaximize_unfocused_pressed"/>
</frame_style>

<frame_style name="utility_focused" parent="normal_focused">
	<piece position="title" draw_ops="draw_title_text_normal"/>
	<piece position="titlebar" draw_ops="draw_title"/>
	<button function="menu" state="normal" draw_ops="menu_focused_normal"/>
	<button function="menu" state="prelight" draw_ops="menu_focused_prelight"/>
	<button function="menu" state="pressed" draw_ops="menu_focused_pressed"/>
	<button function="close" state="normal" draw_ops="close_focused_normal"/>
	<button function="close" state="prelight" draw_ops="close_focused_prelight"/>
	<button function="close" state="pressed" draw_ops="close_focused_pressed"/>
</frame_style>

